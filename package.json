{
  "name": "klotski",
  "version": "0.0.1",
  "description": "The JavaScript algorithm for solving klotski game.",
  "main": "index.js",
  "repository": "https://github.com/jeantimex/klotski.git",
  "author": "jeantimex <jean.timex@gmail.com>",
  "license": "MIT",
  "scripts": {
    "flow": "./node_modules/.bin/flow",
    "lint": "npm run flow && eslint src",
    "test": "npm run lint && cross-env NODE_ENV=test nyc mocha test/**/*-test.js --compilers js:babel-register --require babel-polyfill",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "webpack -p",
    "start": "webpack && node build/main.js"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.2",
    "eslint": "^4.3.0",
    "eslint-plugin-prettier": "^2.1.2",
    "flow-bin": "^0.52.0",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "prettier": "^1.5.3",
    "webpack": "^3.5.2"
  }
}
